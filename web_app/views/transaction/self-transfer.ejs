        <div class="container" style="padding-top: 10px;">

            <form id="self-transfer-form">
                <div class="row">
                    <div class="col">
                            <label class="form-check-label" for="fund-amount">
                                Fund Amount
                            </label>

                            <input id="fund-amount" type="text" readonly>
                    </div>
                </div>
                <div id="Transfer-from-div" class="mb-3">
                    <label for="transfer-from" class="form-label">Transfer From</label>
                    <select id="transfer-from" class="form-control" required>

                    </select>
                </div>

                <div id="Transfer-to-div" class="mb-3">
                    <label for="transfer-to" class="form-label">Transfer To</label>
                    <select id="transfer-to" class="form-control" required>

                    </select>
                </div>
                <div class="mb-3">
                    <label for="amount" class="form-label">Amount</label>
                    <input type="number" class="form-control" id="amount" required>
                </div>

                <div class='mb-3'>
                    <label for="self-transfer-date" class="form-label">Date</label>
                    <input type="text" id="self-transfer-date" class="form-control" required>
                </div>
                
                <button type="submit" class="btn btn-primary">Submit Data</button>
            </form>
        </div>
        </main>

